    android:id="@+id/nav_graph"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.example.chessclock.HomeFragment"
        android:label="HomeFragment" >
        <action
            android:id="@+id/action_homeFragment_to_chessFormats"
            app:destination="@id/chessFormats" />
    </fragment>
    <fragment
        android:id="@+id/chessFormats"
        android:name="com.example.chessclock.ChessFormats"
        android:label="ChessFormats" >
        <action
            android:id="@+id/action_chessFormats_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>
</navigation>